      <def name="Major Depressive Disorder Encounter" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="ValidEncounter">
               <expression xsi:type="Union">
                  <operand xsi:type="Union">
                     <operand xsi:type="Union">
                        <operand xsi:type="Union">
                           <operand dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                              <codes name="Psych Visit Diagnostic Evaluation" preserve="true" xsi:type="ValueSetRef"/>
                           </operand>
                           <operand dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                              <codes name="Emergency Department Visit" preserve="true" xsi:type="ValueSetRef"/>
                           </operand>
                        </operand>
                        <operand xsi:type="Union">
                           <operand dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                              <codes name="Office Visit" preserve="true" xsi:type="ValueSetRef"/>
                           </operand>
                           <operand dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                              <codes name="Outpatient Consultation" preserve="true" xsi:type="ValueSetRef"/>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="Union">
                        <operand dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                           <codes name="Psych Visit Psychotherapy" preserve="true" xsi:type="ValueSetRef"/>
                        </operand>
                        <operand dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                           <codes name="Psychoanalysis" preserve="true" xsi:type="ValueSetRef"/>
                        </operand>
                     </operand>
                  </operand>
                  <operand dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Telehealth Services" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="Equal">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="ValidEncounter" xsi:type="Property"/>
                  </operand>
                  <operand valueType="t:String" value="finished" xsi:type="Literal"/>
               </operand>
               <operand xsi:type="Or">
                  <operand xsi:type="AnyInValueSet">
                     <codes xsi:type="Query">
                        <source alias="$this">
                           <expression path="reasonCode" scope="ValidEncounter" xsi:type="Property"/>
                        </source>
                        <return distinct="false">
                           <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </codes>
                     <valueset name="Major Depressive Disorder Active" preserve="true"/>
                  </operand>
                  <operand xsi:type="Exists">
                     <operand xsi:type="Query">
                        <source alias="EncounterDiagnosis">
                           <expression name="EncounterDiagnosis" libraryName="CQMCommon" xsi:type="FunctionRef">
                              <operand name="ValidEncounter" xsi:type="AliasRef"/>
                           </expression>
                        </source>
                        <where xsi:type="InValueSet">
                           <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="code" scope="EncounterDiagnosis" xsi:type="Property"/>
                           </code>
                           <valueset name="Major Depressive Disorder Active" preserve="true"/>
                        </where>
                     </operand>
                  </operand>
               </operand>
            </where>
         </expression>
      </def>
      <def name="New or Recurrent Major Depressive Disorder Encounter" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="NewOrRecurrentMDDEncounter">
               <expression name="Major Depressive Disorder Encounter" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="PriorMDDEpisodeEncounter" xsi:type="Without">
               <expression name="Major Depressive Disorder Encounter" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="And">
                  <operand xsi:type="Not">
                     <operand xsi:type="Equivalent">
                        <operand name="PriorMDDEpisodeEncounter" xsi:type="AliasRef"/>
                        <operand name="NewOrRecurrentMDDEncounter" xsi:type="AliasRef"/>
                     </operand>
                  </operand>
                  <operand xsi:type="And">
                     <operand precision="Day" xsi:type="In">
                        <operand xsi:type="End">
                           <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="period" scope="PriorMDDEpisodeEncounter" xsi:type="Property"/>
                           </operand>
                        </operand>
                        <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                           <low xsi:type="Subtract">
                              <operand xsi:type="Start">
                                 <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                    <operand path="period" scope="NewOrRecurrentMDDEncounter" xsi:type="Property"/>
                                 </operand>
                              </operand>
                              <operand value="104" unit="days" xsi:type="Quantity"/>
                           </low>
                           <high xsi:type="Start">
                              <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="period" scope="NewOrRecurrentMDDEncounter" xsi:type="Property"/>
                              </operand>
                           </high>
                        </operand>
                     </operand>
                     <operand xsi:type="Not">
                        <operand xsi:type="IsNull">
                           <operand xsi:type="Start">
                              <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="period" scope="NewOrRecurrentMDDEncounter" xsi:type="Property"/>
                              </operand>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
            <where xsi:type="IncludedIn">
               <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                  <operand path="period" scope="NewOrRecurrentMDDEncounter" xsi:type="Property"/>
               </operand>
               <operand name="Measurement Period" xsi:type="ParameterRef"/>
            </where>
         </expression>
      </def>
      <def name="Initial Population" context="Patient" accessLevel="Public">
         <annotation xsi:type="a:Annotation">
            <a:t name="description" value="New or recurrent major depressive disorder for patients 17 &#xa;years or older at the start of the measurement period."/>
         </annotation>
         <expression xsi:type="Query">
            <source alias="NewOrRecurrentMDDEncounter">
               <expression name="New or Recurrent Major Depressive Disorder Encounter" xsi:type="ExpressionRef"/>
            </source>
            <where xsi:type="GreaterOrEqual">
               <operand precision="Year" xsi:type="CalculateAgeAt">
                  <operand path="birthDate" xsi:type="Property">
                     <source name="Patient" xsi:type="ExpressionRef"/>
                  </operand>
                  <operand xsi:type="DateFrom">
                     <operand xsi:type="Start">
                        <operand name="Measurement Period" xsi:type="ParameterRef"/>
                     </operand>
                  </operand>
               </operand>
               <operand valueType="t:Integer" value="17" xsi:type="Literal"/>
            </where>
         </expression>
      </def>
      <def name="Denominator" context="Patient" accessLevel="Public">
         <expression name="Initial Population" xsi:type="ExpressionRef"/>
      </def>
      <def name="Numerator" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="NewOrRecurrentMDDEncounter">
               <expression name="New or Recurrent Major Depressive Disorder Encounter" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="SuicideRiskAssessment" xsi:type="With">
               <expression dataType="fhir:Procedure" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-procedure" codeProperty="code" codeComparator="~" xsi:type="Retrieve">
                  <codes xsi:type="ToList">
                     <operand name="Suicide risk assessment (procedure)" xsi:type="CodeRef"/>
                  </codes>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="Equal">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="SuicideRiskAssessment" xsi:type="Property"/>
                     </operand>
                     <operand valueType="t:String" value="completed" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="IncludedIn">
                     <operand name="ToInterval" libraryName="QICoreCommon" xsi:type="FunctionRef">
                        <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="performed" scope="SuicideRiskAssessment" xsi:type="Property"/>
                        </operand>
                     </operand>
                     <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="period" scope="NewOrRecurrentMDDEncounter" xsi:type="Property"/>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
   </statements>
