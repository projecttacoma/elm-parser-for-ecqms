            <source alias="CupToDiscExamNotPerformed">
               <expression xsi:type="Union">
                  <operand dataType="fhir:Observation" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-observationnotdone" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Cup to Disc Ratio" preserve="true" xsi:type="ValueSetRef"/>
