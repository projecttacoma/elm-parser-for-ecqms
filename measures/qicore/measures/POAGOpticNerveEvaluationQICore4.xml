                  <operand dataType="fhir:Observation" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-observationnotdone" codeProperty="code" codeComparator="~" xsi:type="Retrieve">
                     <codes name="Cup to Disc Ratio" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
               </expression>
            </source>
            <relationship alias="EncounterWithPOAG" xsi:type="With">
               <expression name="Primary Open Angle Glaucoma Encounter" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="In">
                  <operand path="value" xsi:type="Property">
                     <source path="issued" scope="CupToDiscExamNotPerformed" xsi:type="Property"/>
                  </operand>
                  <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="period" scope="EncounterWithPOAG" xsi:type="Property"/>
                  </operand>
               </suchThat>
            </relationship>
            <where xsi:type="InValueSet">
               <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                  <operand xsi:type="SingletonFrom">
                     <operand xsi:type="Query">
                        <source alias="$this">
                           <expression path="extension" xsi:type="Property">
                              <source name="CupToDiscExamNotPerformed" xsi:type="AliasRef"/>
                           </expression>
                        </source>
                        <where xsi:type="Equal">
                           <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="url" scope="$this" xsi:type="Property"/>
                           </operand>
                           <operand valueType="t:String" value="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-notDoneReason" xsi:type="Literal"/>
                        </where>
                        <return distinct="false">
                           <expression path="value" xsi:type="Property">
                              <source name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </operand>
                  </operand>
               </code>
               <valueset name="Medical Reason" preserve="true"/>
            </where>
         </expression>
      </def>
