      <def name="Essential Hypertension Diagnosis" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="Hypertension">
               <expression dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                  <codes name="Essential Hypertension" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="Equal">
                  <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="verificationStatus" scope="Hypertension" xsi:type="Property"/>
                  </operand>
                  <operand xsi:type="ToConcept">
                     <operand name="confirmed" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                  </operand>
               </operand>
               <operand xsi:type="Or">
                  <operand xsi:type="And">
                     <operand xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="recordedDate" scope="Hypertension" xsi:type="Property"/>
                        </operand>
                        <operand lowClosed="true" highClosed="true" xsi:type="Interval">
                           <low xsi:type="Start">
                              <operand name="Measurement Period" xsi:type="ParameterRef"/>
                           </low>
                           <high xsi:type="Add">
                              <operand xsi:type="Start">
                                 <operand name="Measurement Period" xsi:type="ParameterRef"/>
                              </operand>
                              <operand value="6" unit="months" xsi:type="Quantity"/>
                           </high>
                        </operand>
                     </operand>
                     <operand xsi:type="Not">
                        <operand xsi:type="IsNull">
                           <operand xsi:type="Start">
                              <operand name="Measurement Period" xsi:type="ParameterRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
                  <operand xsi:type="Before">
                     <operand path="value" xsi:type="Property">
                        <source path="recordedDate" scope="Hypertension" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="End">
                        <operand name="Measurement Period" xsi:type="ParameterRef"/>
                     </operand>
                  </operand>
               </operand>
            </where>
         </expression>
      </def>
      <def name="Initial Population" context="Patient" accessLevel="Public">
         <expression xsi:type="And">
            <operand xsi:type="And">
               <operand xsi:type="And">
                  <operand xsi:type="GreaterOrEqual">
                     <operand precision="Year" xsi:type="CalculateAgeAt">
                        <operand xsi:type="ToDateTime">
                           <operand xsi:type="ToDate">
                              <operand xsi:type="ToDateTime">
                                 <operand path="value" xsi:type="Property">
                                    <source path="birthDate" xsi:type="Property">
                                       <source name="Patient" xsi:type="ExpressionRef"/>
                                    </source>
                                 </operand>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="End">
                           <operand name="Measurement Period" xsi:type="ParameterRef"/>
                        </operand>
                     </operand>
                     <operand valueType="t:Integer" value="18" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="LessOrEqual">
                     <operand precision="Year" xsi:type="CalculateAgeAt">
                        <operand xsi:type="ToDateTime">
                           <operand xsi:type="ToDate">
                              <operand xsi:type="ToDateTime">
                                 <operand path="value" xsi:type="Property">
                                    <source path="birthDate" xsi:type="Property">
                                       <source name="Patient" xsi:type="ExpressionRef"/>
                                    </source>
                                 </operand>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="End">
                           <operand name="Measurement Period" xsi:type="ParameterRef"/>
                        </operand>
                     </operand>
                     <operand valueType="t:Integer" value="85" xsi:type="Literal"/>
                  </operand>
               </operand>
               <operand xsi:type="Exists">
                  <operand name="Essential Hypertension Diagnosis" xsi:type="ExpressionRef"/>
               </operand>
            </operand>
            <operand xsi:type="Exists">
               <operand name="Qualifying Encounters" libraryName="AdultOutpatientEncounters" xsi:type="ExpressionRef"/>
            </operand>
         </expression>
      </def>
      <def name="Pregnancy Or Renal Diagnosis Exclusions" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="PregnancyESRDDiagnosis">
               <expression xsi:type="Union">
                  <operand xsi:type="Union">
                     <operand dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Pregnancy" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                     <operand dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="End Stage Renal Disease" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                  </operand>
                  <operand xsi:type="Union">
                     <operand dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Kidney Transplant Recipient" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                     <operand dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Chronic Kidney Disease, Stage 5" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                  </operand>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="Equal">
                  <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="verificationStatus" scope="PregnancyESRDDiagnosis" xsi:type="Property"/>
                  </operand>
                  <operand xsi:type="ToConcept">
                     <operand name="confirmed" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                  </operand>
               </operand>
               <operand xsi:type="Before">
                  <operand path="value" xsi:type="Property">
                     <source path="recordedDate" scope="PregnancyESRDDiagnosis" xsi:type="Property"/>
                  </operand>
                  <operand xsi:type="End">
                     <operand name="Measurement Period" xsi:type="ParameterRef"/>
                  </operand>
               </operand>
            </where>
         </expression>
      </def>
      <def name="Denominator" context="Patient" accessLevel="Public">
         <expression name="Initial Population" xsi:type="ExpressionRef"/>
      </def>
      <def name="End Stage Renal Disease Encounter" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="ESRDEncounter">
               <expression dataType="fhir:Encounter" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-encounter" codeProperty="type" codeComparator="in" xsi:type="Retrieve">
                  <codes name="ESRD Monthly Outpatient Services" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="In">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="ESRDEncounter" xsi:type="Property"/>
                  </operand>
                  <operand xsi:type="List">
                     <element valueType="t:String" value="arrived" xsi:type="Literal"/>
                     <element valueType="t:String" value="triaged" xsi:type="Literal"/>
                     <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                     <element valueType="t:String" value="onleave" xsi:type="Literal"/>
                     <element valueType="t:String" value="finished" xsi:type="Literal"/>
                  </operand>
               </operand>
               <operand xsi:type="Before">
                  <operand xsi:type="Start">
                     <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="period" scope="ESRDEncounter" xsi:type="Property"/>
                     </operand>
                  </operand>
                  <operand xsi:type="End">
                     <operand name="Measurement Period" xsi:type="ParameterRef"/>
                  </operand>
               </operand>
            </where>
         </expression>
      </def>
      <def name="End Stage Renal Disease Procedures" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="ESRDProcedure">
               <expression xsi:type="Union">
                  <operand xsi:type="Union">
                     <operand dataType="fhir:Procedure" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-procedure" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Vascular Access for Dialysis" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                     <operand dataType="fhir:Procedure" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-procedure" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Kidney Transplant" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                  </operand>
                  <operand dataType="fhir:Procedure" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-procedure" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Dialysis Services" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="Equal">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="ESRDProcedure" xsi:type="Property"/>
                  </operand>
                  <operand valueType="t:String" value="completed" xsi:type="Literal"/>
               </operand>
               <operand xsi:type="Before">
                  <operand xsi:type="Start">
                     <operand xsi:type="As">
                        <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="performed" scope="ESRDProcedure" xsi:type="Property"/>
                        </operand>
                        <asTypeSpecifier xsi:type="IntervalTypeSpecifier">
                           <pointType name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
                        </asTypeSpecifier>
                     </operand>
                  </operand>
                  <operand xsi:type="End">
                     <operand name="Measurement Period" xsi:type="ParameterRef"/>
                  </operand>
               </operand>
            </where>
         </expression>
      </def>
   </statements>
</library>