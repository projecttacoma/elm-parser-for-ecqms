   </includes>
   <parameters>
      <def name="Measurement Period" accessLevel="Public">
         <parameterTypeSpecifier xsi:type="IntervalTypeSpecifier">
            <pointType name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </parameterTypeSpecifier>
      </def>
   </parameters>
   <valueSets>
      <def name="Anticoagulant Therapy" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.200" accessLevel="Public"/>
      <def name="Atrial Ablation" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.203" accessLevel="Public"/>
      <def name="Atrial Fibrillation/Flutter" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.202" accessLevel="Public"/>
      <def name="Comfort Measures" id="http://cts.nlm.nih.gov/fhir/ValueSet/1.3.6.1.4.1.33895.1.3.0.45" accessLevel="Public"/>
      <def name="Discharge To Acute Care Facility" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.87" accessLevel="Public"/>
      <def name="Discharged to Health Care Facility for Hospice Care" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.207" accessLevel="Public"/>
      <def name="Discharged to Home for Hospice Care" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.209" accessLevel="Public"/>
      <def name="Emergency Department Visit" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.292" accessLevel="Public"/>
      <def name="Hemorrhagic Stroke" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.212" accessLevel="Public"/>
      <def name="Ischemic Stroke" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.247" accessLevel="Public"/>
      <def name="Left Against Medical Advice" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.308" accessLevel="Public"/>
      <def name="Medical Reason" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.473" accessLevel="Public"/>
      <def name="Non-Elective Inpatient Encounter" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.424" accessLevel="Public"/>
      <def name="Patient Expired" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.309" accessLevel="Public"/>
      <def name="Patient Refusal" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.117.1.7.1.93" accessLevel="Public"/>
   </valueSets>
   <contexts>
      <def name="Patient"/>
   </contexts>
   <statements>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand dataType="fhir:Patient" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="Initial Population" context="Patient" accessLevel="Public">
         <expression name="Encounter with Principal Diagnosis and Age" libraryName="TJC" xsi:type="ExpressionRef"/>
      </def>
      <def name="Encounter with Atrial Ablation Procedure" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <where xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="AtrialAblation">
                     <expression dataType="fhir:Procedure" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-procedure" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Atrial Ablation" preserve="true" xsi:type="ValueSetRef"/>
                     </expression>
                  </source>
                  <where xsi:type="And">
                     <operand xsi:type="Before">
                        <operand xsi:type="Start">
                           <operand name="ToInterval" libraryName="QICoreCommon" xsi:type="FunctionRef">
                              <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="performed" scope="AtrialAblation" xsi:type="Property"/>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="Start">
                           <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="period" scope="IschemicStrokeEncounter" xsi:type="Property"/>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="Equal">
                        <operand path="value" xsi:type="Property">
                           <source path="status" scope="AtrialAblation" xsi:type="Property"/>
                        </operand>
                        <operand valueType="t:String" value="completed" xsi:type="Literal"/>
                     </operand>
                  </where>
               </operand>
            </where>
         </expression>
      </def>
      <def name="Encounter with a History of Atrial Fibrillation or Flutter" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="AtrialFibrillationFlutter" xsi:type="With">
               <expression dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                  <codes name="Atrial Fibrillation/Flutter" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
               <suchThat xsi:type="SameOrBefore">
                  <operand xsi:type="Start">
                     <operand name="ToInterval" libraryName="QICoreCommon" xsi:type="FunctionRef">
                        <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="onset" scope="AtrialFibrillationFlutter" xsi:type="Property"/>
                        </operand>
                     </operand>
                  </operand>
                  <operand xsi:type="End">
                     <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="period" scope="IschemicStrokeEncounter" xsi:type="Property"/>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Encounter with Current Diagnosis Code of Atrial Fibrillation or Flutter" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <where xsi:type="AnyInValueSet">
               <codes xsi:type="Query">
                  <source alias="$this">
                     <expression name="EncounterDiagnosis" libraryName="CQMCommon" xsi:type="FunctionRef">
                        <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                     </expression>
                  </source>
                  <where xsi:type="Not">
                     <operand xsi:type="IsNull">
                        <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="code" xsi:type="Property">
                              <source name="$this" xsi:type="AliasRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </where>
                  <return distinct="false">
                     <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="code" xsi:type="Property">
                           <source name="$this" xsi:type="AliasRef"/>
                        </operand>
                     </expression>
                  </return>
               </codes>
               <valueset name="Atrial Fibrillation/Flutter" preserve="true"/>
            </where>
         </expression>
      </def>
      <def name="Denominator" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand xsi:type="Union">
               <operand name="Encounter with Atrial Ablation Procedure" xsi:type="ExpressionRef"/>
               <operand name="Encounter with a History of Atrial Fibrillation or Flutter" xsi:type="ExpressionRef"/>
            </operand>
            <operand name="Encounter with Current Diagnosis Code of Atrial Fibrillation or Flutter" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="Encounter with Comfort Measures during Hospitalization for Patients with Documented Atrial Fibrillation or Flutter" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="Encounter">
               <expression name="Denominator" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="ComfortMeasure" xsi:type="With">
               <expression name="Intervention Comfort Measures" libraryName="TJC" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="In">
                  <operand xsi:type="Coalesce">
                     <operand xsi:type="Start">
                        <operand name="ToInterval" libraryName="QICoreCommon" xsi:type="FunctionRef">
                           <operand path="FHIRHelpers.ToValue(%value)" scope="ComfortMeasure" xsi:type="Property"/>
                        </operand>
                     </operand>
                     <operand path="%value.value" scope="ComfortMeasure" xsi:type="Property"/>
                  </operand>
                  <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                     <operand name="Encounter" xsi:type="AliasRef"/>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Denominator Exclusions" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand xsi:type="Query">
               <source alias="Encounter">
                  <expression name="Denominator" xsi:type="ExpressionRef"/>
               </source>
               <where xsi:type="And">
                  <operand xsi:type="Equal">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="Encounter" xsi:type="Property"/>
                     </operand>
                     <operand valueType="t:String" value="finished" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="Or">
                     <operand xsi:type="Or">
                        <operand xsi:type="Or">
                           <operand xsi:type="Or">
                              <operand xsi:type="InValueSet">
                                 <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                    <operand path="dischargeDisposition" xsi:type="Property">
                                       <source path="hospitalization" scope="Encounter" xsi:type="Property"/>
                                    </operand>
                                 </code>
                                 <valueset name="Discharge To Acute Care Facility" preserve="true"/>
                              </operand>
                              <operand xsi:type="InValueSet">
                                 <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                    <operand path="dischargeDisposition" xsi:type="Property">
                                       <source path="hospitalization" scope="Encounter" xsi:type="Property"/>
                                    </operand>
                                 </code>
                                 <valueset name="Left Against Medical Advice" preserve="true"/>
                              </operand>
                           </operand>
                           <operand xsi:type="InValueSet">
                              <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="dischargeDisposition" xsi:type="Property">
                                    <source path="hospitalization" scope="Encounter" xsi:type="Property"/>
                                 </operand>
                              </code>
                              <valueset name="Patient Expired" preserve="true"/>
                           </operand>
                        </operand>
                        <operand xsi:type="InValueSet">
                           <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="dischargeDisposition" xsi:type="Property">
                                 <source path="hospitalization" scope="Encounter" xsi:type="Property"/>
                              </operand>
                           </code>
                           <valueset name="Discharged to Home for Hospice Care" preserve="true"/>
                        </operand>
                     </operand>
                     <operand xsi:type="InValueSet">
                        <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="dischargeDisposition" xsi:type="Property">
                              <source path="hospitalization" scope="Encounter" xsi:type="Property"/>
                           </operand>
                        </code>
                        <valueset name="Discharged to Health Care Facility for Hospice Care" preserve="true"/>
                     </operand>
                  </operand>
               </where>
            </operand>
            <operand name="Encounter with Comfort Measures during Hospitalization for Patients with Documented Atrial Fibrillation or Flutter" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="Numerator" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="Encounter">
               <expression name="Denominator" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="DischargeAnticoagulant" xsi:type="With">
               <expression xsi:type="Union">
                  <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Anticoagulant Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
                  <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Anticoagulant Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="And">
                     <operand xsi:type="And">
                        <operand xsi:type="And">
                           <operand xsi:type="In">
                              <operand path="value" xsi:type="Property">
                                 <source path="authoredOn" scope="DischargeAnticoagulant" xsi:type="Property"/>
                              </operand>
                              <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="period" scope="Encounter" xsi:type="Property"/>
                              </operand>
                           </operand>
                           <operand xsi:type="In">
                              <operand path="value" xsi:type="Property">
                                 <source path="status" scope="DischargeAnticoagulant" xsi:type="Property"/>
                              </operand>
                              <operand xsi:type="List">
                                 <element valueType="t:String" value="active" xsi:type="Literal"/>
                                 <element valueType="t:String" value="completed" xsi:type="Literal"/>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="Not">
                           <operand xsi:type="IsTrue">
                              <operand path="value" xsi:type="Property">
                                 <source path="doNotPerform" scope="DischargeAnticoagulant" xsi:type="Property"/>
                              </operand>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="Exists">
                        <operand xsi:type="Query">
                           <source alias="C">
                              <expression xsi:type="Query">
                                 <source alias="$this">
                                    <expression path="category" scope="DischargeAnticoagulant" xsi:type="Property"/>
                                 </source>
                                 <return distinct="false">
                                    <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                       <operand name="$this" xsi:type="AliasRef"/>
                                    </expression>
                                 </return>
                              </expression>
                           </source>
                           <where xsi:type="Or">
                              <operand xsi:type="Equivalent">
                                 <operand name="C" xsi:type="AliasRef"/>
                                 <operand xsi:type="ToConcept">
                                    <operand name="Community" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                                 </operand>
                              </operand>
                              <operand xsi:type="Equivalent">
                                 <operand name="C" xsi:type="AliasRef"/>
                                 <operand xsi:type="ToConcept">
                                    <operand name="Discharge" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                                 </operand>
                              </operand>
                           </where>
                        </operand>
                     </operand>
                  </operand>
                  <operand xsi:type="Equal">
                     <operand path="value" xsi:type="Property">
                        <source path="intent" scope="DischargeAnticoagulant" xsi:type="Property"/>
                     </operand>
                     <operand valueType="t:String" value="order" xsi:type="Literal"/>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Reason for Not Giving Anticoagulant at Discharge" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="NoAnticoagulant">
               <expression dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                  <codes name="Anticoagulant Therapy" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="And">
                  <operand xsi:type="Equal">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="NoAnticoagulant" xsi:type="Property"/>
                     </operand>
                     <operand valueType="t:String" value="completed" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="IsTrue">
                     <operand path="value" xsi:type="Property">
                        <source path="doNotPerform" scope="NoAnticoagulant" xsi:type="Property"/>
                     </operand>
                  </operand>
               </operand>
               <operand xsi:type="Or">
                  <operand xsi:type="AnyInValueSet">
                     <codes xsi:type="Query">
                        <source alias="$this">
                           <expression path="reasonCode" scope="NoAnticoagulant" xsi:type="Property"/>
                        </source>
                        <return distinct="false">
                           <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </codes>
                     <valueset name="Medical Reason" preserve="true"/>
                  </operand>
                  <operand xsi:type="AnyInValueSet">
                     <codes xsi:type="Query">
                        <source alias="$this">
                           <expression path="reasonCode" scope="NoAnticoagulant" xsi:type="Property"/>
                        </source>
                        <return distinct="false">
                           <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </codes>
                     <valueset name="Patient Refusal" preserve="true"/>
                  </operand>
               </operand>
            </where>
         </expression>
      </def>
      <def name="Denominator Exceptions" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="Encounter">
               <expression name="Denominator" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="NoDischargeAnticoagulant" xsi:type="With">
               <expression name="Reason for Not Giving Anticoagulant at Discharge" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="In">
                  <operand path="value" xsi:type="Property">
                     <source path="authoredOn" scope="NoDischargeAnticoagulant" xsi:type="Property"/>
                  </operand>
                  <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="period" scope="Encounter" xsi:type="Property"/>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="SDE Ethnicity" context="Patient" accessLevel="Public">
         <expression name="SDE Ethnicity" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
      <def name="SDE Payer" context="Patient" accessLevel="Public">
         <expression name="SDE Payer" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
      <def name="SDE Race" context="Patient" accessLevel="Public">
         <expression name="SDE Race" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
      <def name="SDE Sex" context="Patient" accessLevel="Public">
         <expression name="SDE Sex" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
   </statements>
</library>