   </includes>
   <parameters>
      <def name="Measurement Period" accessLevel="Public">
         <default lowClosed="true" highClosed="false" xsi:type="Interval">
            <low xsi:type="DateTime">
               <year valueType="t:Integer" value="2022" xsi:type="Literal"/>
               <month valueType="t:Integer" value="1" xsi:type="Literal"/>
               <day valueType="t:Integer" value="1" xsi:type="Literal"/>
               <hour valueType="t:Integer" value="0" xsi:type="Literal"/>
               <minute valueType="t:Integer" value="0" xsi:type="Literal"/>
               <second valueType="t:Integer" value="0" xsi:type="Literal"/>
               <millisecond valueType="t:Integer" value="0" xsi:type="Literal"/>
            </low>
            <high xsi:type="DateTime">
               <year valueType="t:Integer" value="2023" xsi:type="Literal"/>
               <month valueType="t:Integer" value="1" xsi:type="Literal"/>
               <day valueType="t:Integer" value="1" xsi:type="Literal"/>
               <hour valueType="t:Integer" value="0" xsi:type="Literal"/>
               <minute valueType="t:Integer" value="0" xsi:type="Literal"/>
               <second valueType="t:Integer" value="0" xsi:type="Literal"/>
               <millisecond valueType="t:Integer" value="0" xsi:type="Literal"/>
            </high>
         </default>
         <parameterTypeSpecifier xsi:type="IntervalTypeSpecifier">
            <pointType name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </parameterTypeSpecifier>
      </def>
   </parameters>
   <codeSystems>
      <def name="SNOMEDCT" id="http://snomed.info/sct" accessLevel="Public"/>
      <def name="RXNORM" id="http://www.nlm.nih.gov/research/umls/rxnorm" accessLevel="Public"/>
   </codeSystems>
   <valueSets>
      <def name="ACE Inhibitor or ARB Ingredient" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.1489" accessLevel="Public"/>
      <def name="ACE Inhibitor or ARB or ARNI" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.1139" accessLevel="Public"/>
      <def name="Allergy to ACE Inhibitor or ARB" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.1211" accessLevel="Public"/>
      <def name="Intolerance to ACE Inhibitor or ARB" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.1212" accessLevel="Public"/>
      <def name="Medical Reason" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.1007" accessLevel="Public"/>
      <def name="Patient Reason" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.1008" accessLevel="Public"/>
      <def name="Patient Reason for ACE Inhibitor or ARB Decline" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.1140" accessLevel="Public"/>
      <def name="Pregnancy" id="http://cts.nlm.nih.gov/fhir/ValueSet/2.16.840.1.113883.3.526.3.378" accessLevel="Public"/>
   </valueSets>
   <codes>
      <def name="Acute renal failure caused by angiotensin-converting-enzyme inhibitor (disorder)" id="422593004" display="Acute renal failure caused by angiotensin-converting-enzyme inhibitor (disorder)" accessLevel="Public">
         <codeSystem name="SNOMEDCT"/>
      </def>
      <def name="sacubitril" id="1656328" display="sacubitril" accessLevel="Public">
         <codeSystem name="RXNORM"/>
      </def>
      <def name="Substance with angiotensin II receptor antagonist mechanism of action (substance)" id="372913009" display="Substance with angiotensin II receptor antagonist mechanism of action (substance)" accessLevel="Public">
         <codeSystem name="SNOMEDCT"/>
      </def>
      <def name="Substance with angiotensin-converting enzyme inhibitor mechanism of action (substance)" id="372733002" display="Substance with angiotensin-converting enzyme inhibitor mechanism of action (substance)" accessLevel="Public">
         <codeSystem name="SNOMEDCT"/>
      </def>
      <def name="Substance with neprilysin inhibitor mechanism of action (substance)" id="786886009" display="Substance with neprilysin inhibitor mechanism of action (substance)" accessLevel="Public">
         <codeSystem name="SNOMEDCT"/>
      </def>
   </codes>
   <contexts>
      <def name="Patient"/>
   </contexts>
   <statements>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand dataType="fhir:Patient" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="SDE Ethnicity" context="Patient" accessLevel="Public">
         <expression name="SDE Ethnicity" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
      <def name="SDE Payer" context="Patient" accessLevel="Public">
         <expression name="SDE Payer" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
      <def name="SDE Race" context="Patient" accessLevel="Public">
         <expression name="SDE Race" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
      <def name="SDE Sex" context="Patient" accessLevel="Public">
         <expression name="SDE Sex" libraryName="SDE" xsi:type="ExpressionRef"/>
      </def>
      <def name="Initial Population" context="Patient" accessLevel="Public">
         <expression xsi:type="And">
            <operand xsi:type="And">
               <operand xsi:type="GreaterOrEqual">
                  <operand precision="Year" xsi:type="CalculateAgeAt">
                     <operand path="value" xsi:type="Property">
                        <source path="birthDate" xsi:type="Property">
                           <source name="Patient" xsi:type="ExpressionRef"/>
                        </source>
                     </operand>
                     <operand xsi:type="DateFrom">
                        <operand xsi:type="Start">
                           <operand name="Measurement Period" xsi:type="ParameterRef"/>
                        </operand>
                     </operand>
                  </operand>
                  <operand valueType="t:Integer" value="18" xsi:type="Literal"/>
               </operand>
               <operand xsi:type="GreaterOrEqual">
                  <operand xsi:type="Count">
                     <source name="Qualifying Encounter During Measurement Period" libraryName="AHA" xsi:type="ExpressionRef"/>
                  </operand>
                  <operand valueType="t:Integer" value="2" xsi:type="Literal"/>
               </operand>
            </operand>
            <operand xsi:type="Exists">
               <operand name="Heart Failure Outpatient Encounter" libraryName="AHA" xsi:type="ExpressionRef"/>
            </operand>
         </expression>
      </def>
      <def name="Denominator" context="Patient" accessLevel="Public">
         <expression xsi:type="And">
            <operand name="Initial Population" xsi:type="ExpressionRef"/>
            <operand xsi:type="Exists">
               <operand name="Heart Failure Outpatient Encounter with History of Moderate or Severe LVSD" libraryName="AHA" xsi:type="ExpressionRef"/>
            </operand>
         </expression>
      </def>
      <def name="Denominator Exclusions" context="Patient" accessLevel="Public">
         <expression xsi:type="Or">
            <operand xsi:type="Or">
               <operand xsi:type="Or">
                  <operand name="Has Heart Transplant" libraryName="AHA" xsi:type="ExpressionRef"/>
                  <operand name="Has Heart Transplant Related Diagnosis" libraryName="AHA" xsi:type="ExpressionRef"/>
               </operand>
               <operand name="Has Left Ventricular Assist Device" libraryName="AHA" xsi:type="ExpressionRef"/>
            </operand>
            <operand name="Has Left Ventricular Assist Device Related Diagnosis" libraryName="AHA" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="Has ACEI or ARB or ARNI Ordered" context="Patient" accessLevel="Public">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="ACEIOrARBOrARNI">
                  <expression xsi:type="Union">
                     <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                        <codes name="ACE Inhibitor or ARB or ARNI" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                     <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                        <codes name="ACE Inhibitor or ARB or ARNI" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                  </expression>
               </source>
               <where name="isOrderedDuringHeartFailureOutpatientEncounter" libraryName="AHA" xsi:type="FunctionRef">
                  <operand name="ACEIOrARBOrARNI" xsi:type="AliasRef"/>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Is Currently Taking ACEI or ARB or ARNI" context="Patient" accessLevel="Public">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="ActiveACEIOrARBOrARNI">
                  <expression xsi:type="Union">
                     <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                        <codes name="ACE Inhibitor or ARB or ARNI" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                     <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                        <codes name="ACE Inhibitor or ARB or ARNI" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                  </expression>
               </source>
               <where name="overlapsAfterHeartFailureOutpatientEncounter" libraryName="AHA" xsi:type="FunctionRef">
                  <operand xsi:type="As">
                     <operand name="ActiveACEIOrARBOrARNI" xsi:type="AliasRef"/>
                     <asTypeSpecifier xsi:type="ChoiceTypeSpecifier">
                        <choice name="fhir:Condition" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:Procedure" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:AllergyIntolerance" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:MedicationRequest" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:Observation" xsi:type="NamedTypeSpecifier"/>
                     </asTypeSpecifier>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Numerator" context="Patient" accessLevel="Public">
         <expression xsi:type="Or">
            <operand name="Has ACEI or ARB or ARNI Ordered" xsi:type="ExpressionRef"/>
            <operand name="Is Currently Taking ACEI or ARB or ARNI" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="Has Medical or Patient Reason for Not Ordering ACEI or ARB or ARNI" context="Patient" accessLevel="Public">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="NoACEIOrARBOrARNIOrdered">
                  <expression dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-mednotrequested" xsi:type="Retrieve"/>
               </source>
               <relationship alias="ModerateOrSevereLVSDHFOutpatientEncounter" xsi:type="With">
                  <expression name="Heart Failure Outpatient Encounter with History of Moderate or Severe LVSD" libraryName="AHA" xsi:type="ExpressionRef"/>
                  <suchThat xsi:type="In">
                     <operand path="value" xsi:type="Property">
                        <source path="authoredOn" scope="NoACEIOrARBOrARNIOrdered" xsi:type="Property"/>
                     </operand>
                     <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="period" scope="ModerateOrSevereLVSDHFOutpatientEncounter" xsi:type="Property"/>
                     </operand>
                  </suchThat>
               </relationship>
               <where xsi:type="And">
                  <operand xsi:type="InValueSet">
                     <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="medication" scope="NoACEIOrARBOrARNIOrdered" xsi:type="Property"/>
                     </code>
                     <valueset name="ACE Inhibitor or ARB or ARNI" preserve="true"/>
                  </operand>
                  <operand xsi:type="Or">
                     <operand xsi:type="Or">
                        <operand xsi:type="InValueSet">
                           <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="reasonCode" scope="NoACEIOrARBOrARNIOrdered" xsi:type="Property"/>
                           </code>
                           <valueset name="Medical Reason" preserve="true"/>
                        </operand>
                        <operand xsi:type="InValueSet">
                           <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="reasonCode" scope="NoACEIOrARBOrARNIOrdered" xsi:type="Property"/>
                           </code>
                           <valueset name="Patient Reason" preserve="true"/>
                        </operand>
                     </operand>
                     <operand xsi:type="InValueSet">
                        <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="reasonCode" scope="NoACEIOrARBOrARNIOrdered" xsi:type="Property"/>
                        </code>
                        <valueset name="Patient Reason for ACE Inhibitor or ARB Decline" preserve="true"/>
                     </operand>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Has Allergy or Intolerance to ACEI or ARB or ARNI Ingredient" context="Patient" accessLevel="Public">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="ACEIOrARBOrARNIAllergyIntolerance">
                  <expression xsi:type="Union">
                     <operand xsi:type="Union">
                        <operand xsi:type="Union">
                           <operand dataType="fhir:AllergyIntolerance" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-allergyintolerance" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                              <codes name="ACE Inhibitor or ARB Ingredient" preserve="true" xsi:type="ValueSetRef"/>
                           </operand>
                           <operand dataType="fhir:AllergyIntolerance" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-allergyintolerance" codeProperty="code" codeComparator="~" xsi:type="Retrieve">
                              <codes xsi:type="ToList">
                                 <operand name="Substance with angiotensin-converting enzyme inhibitor mechanism of action (substance)" xsi:type="CodeRef"/>
                              </codes>
                           </operand>
                        </operand>
                        <operand xsi:type="Union">
                           <operand dataType="fhir:AllergyIntolerance" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-allergyintolerance" codeProperty="code" codeComparator="~" xsi:type="Retrieve">
                              <codes xsi:type="ToList">
                                 <operand name="Substance with angiotensin II receptor antagonist mechanism of action (substance)" xsi:type="CodeRef"/>
                              </codes>
                           </operand>
                           <operand dataType="fhir:AllergyIntolerance" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-allergyintolerance" codeProperty="code" codeComparator="~" xsi:type="Retrieve">
                              <codes xsi:type="ToList">
                                 <operand name="Substance with neprilysin inhibitor mechanism of action (substance)" xsi:type="CodeRef"/>
                              </codes>
                           </operand>
                        </operand>
                     </operand>
                     <operand dataType="fhir:AllergyIntolerance" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-allergyintolerance" codeProperty="code" codeComparator="~" xsi:type="Retrieve">
                        <codes xsi:type="ToList">
                           <operand name="sacubitril" xsi:type="CodeRef"/>
                        </codes>
                     </operand>
                  </expression>
               </source>
               <relationship alias="ModerateOrSevereLVSDHFOutpatientEncounter" xsi:type="With">
                  <expression name="Heart Failure Outpatient Encounter with History of Moderate or Severe LVSD" libraryName="AHA" xsi:type="ExpressionRef"/>
                  <suchThat xsi:type="And">
                     <operand xsi:type="Before">
                        <operand xsi:type="Start">
                           <operand name="toInterval" libraryName="Common" xsi:type="FunctionRef">
                              <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="onset" scope="ACEIOrARBOrARNIAllergyIntolerance" xsi:type="Property"/>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="End">
                           <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="period" scope="ModerateOrSevereLVSDHFOutpatientEncounter" xsi:type="Property"/>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="Or">
                        <operand xsi:type="After">
                           <operand xsi:type="End">
                              <operand name="toInterval" libraryName="Common" xsi:type="FunctionRef">
                                 <operand xsi:type="As">
                                    <operand name="ToQuantity" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                       <operand xsi:type="SingletonFrom">
                                          <operand xsi:type="Query">
                                             <source alias="$this">
                                                <expression path="extension" xsi:type="Property">
                                                   <source name="ACEIOrARBOrARNIAllergyIntolerance" xsi:type="AliasRef"/>
                                                </expression>
                                             </source>
                                             <where xsi:type="Equal">
                                                <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                                   <operand path="url" scope="$this" xsi:type="Property"/>
                                                </operand>
                                                <operand valueType="t:String" value="http://hl7.org/fhir/StructureDefinition/allergyintolerance-resolutionAge" xsi:type="Literal"/>
                                             </where>
                                             <return distinct="false">
                                                <expression path="value" xsi:type="Property">
                                                   <source name="$this" xsi:type="AliasRef"/>
                                                </expression>
                                             </return>
                                          </operand>
                                       </operand>
                                    </operand>
                                    <asTypeSpecifier xsi:type="ChoiceTypeSpecifier">
                                       <choice name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
                                       <choice name="t:Quantity" xsi:type="NamedTypeSpecifier"/>
                                       <choice xsi:type="IntervalTypeSpecifier">
                                          <pointType name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
                                       </choice>
                                       <choice xsi:type="IntervalTypeSpecifier">
                                          <pointType name="t:Quantity" xsi:type="NamedTypeSpecifier"/>
                                       </choice>
                                    </asTypeSpecifier>
                                 </operand>
                              </operand>
                           </operand>
                           <operand xsi:type="End">
                              <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="period" scope="ModerateOrSevereLVSDHFOutpatientEncounter" xsi:type="Property"/>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="IsNull">
                           <operand name="ToQuantity" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand xsi:type="SingletonFrom">
                                 <operand xsi:type="Query">
                                    <source alias="$this">
                                       <expression path="extension" xsi:type="Property">
                                          <source name="ACEIOrARBOrARNIAllergyIntolerance" xsi:type="AliasRef"/>
                                       </expression>
                                    </source>
                                    <where xsi:type="Equal">
                                       <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                          <operand path="url" scope="$this" xsi:type="Property"/>
                                       </operand>
                                       <operand valueType="t:String" value="http://hl7.org/fhir/StructureDefinition/allergyintolerance-resolutionAge" xsi:type="Literal"/>
                                    </where>
                                    <return distinct="false">
                                       <expression path="value" xsi:type="Property">
                                          <source name="$this" xsi:type="AliasRef"/>
                                       </expression>
                                    </return>
                                 </operand>
                              </operand>
                           </operand>
                        </operand>
                     </operand>
                  </suchThat>
               </relationship>
               <where xsi:type="And">
                  <operand xsi:type="Equivalent">
                     <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="clinicalStatus" scope="ACEIOrARBOrARNIAllergyIntolerance" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="ToConcept">
                        <operand name="allergy-active" libraryName="Common" xsi:type="CodeRef"/>
                     </operand>
                  </operand>
                  <operand xsi:type="Equivalent">
                     <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="verificationStatus" scope="ACEIOrARBOrARNIAllergyIntolerance" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="ToConcept">
                        <operand name="allergy-confirmed" libraryName="Common" xsi:type="CodeRef"/>
                     </operand>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Has Diagnosis of Allergy or Intolerance to ACEI or ARB" context="Patient" accessLevel="Public">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="ACEIOrARBAllergyOrIntoleranceDiagnosis">
                  <expression xsi:type="Union">
                     <operand dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Allergy to ACE Inhibitor or ARB" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                     <operand dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Intolerance to ACE Inhibitor or ARB" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                  </expression>
               </source>
               <where name="overlapsAfterHeartFailureOutpatientEncounter" libraryName="AHA" xsi:type="FunctionRef">
                  <operand xsi:type="As">
                     <operand name="ACEIOrARBAllergyOrIntoleranceDiagnosis" xsi:type="AliasRef"/>
                     <asTypeSpecifier xsi:type="ChoiceTypeSpecifier">
                        <choice name="fhir:Condition" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:Procedure" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:AllergyIntolerance" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:MedicationRequest" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:Observation" xsi:type="NamedTypeSpecifier"/>
                     </asTypeSpecifier>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Has Diagnosis of Pregnancy" context="Patient" accessLevel="Public">
         <expression xsi:type="Exists">
            <operand xsi:type="Query">
               <source alias="Pregnancy">
                  <expression dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Pregnancy" preserve="true" xsi:type="ValueSetRef"/>
                  </expression>
               </source>
               <relationship alias="ModerateOrSevereLVSDHFOutpatientEncounter" xsi:type="With">
                  <expression name="Heart Failure Outpatient Encounter with History of Moderate or Severe LVSD" libraryName="AHA" xsi:type="ExpressionRef"/>
                  <suchThat xsi:type="And">
                     <operand xsi:type="In">
                        <operand xsi:type="Start">
                           <operand name="prevalenceInterval" libraryName="Common" xsi:type="FunctionRef">
                              <operand name="Pregnancy" xsi:type="AliasRef"/>
                           </operand>
                        </operand>
                        <operand lowClosed="true" highClosed="true" xsi:type="Interval">
                           <low xsi:type="Subtract">
                              <operand xsi:type="Start">
                                 <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
