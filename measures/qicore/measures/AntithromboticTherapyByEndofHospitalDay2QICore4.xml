   <statements>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand dataType="fhir:Patient" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="Initial Population" context="Patient" accessLevel="Public">
         <expression name="Encounter with Principal Diagnosis and Age" libraryName="TJC" xsi:type="ExpressionRef"/>
      </def>
      <def name="Denominator" context="Patient" accessLevel="Public">
         <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
      </def>
      <def name="Encounter Less Than Two Days" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <where xsi:type="Less">
               <operand name="LengthInDays" libraryName="CQMCommon" xsi:type="FunctionRef">
                  <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                     <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                  </operand>
               </operand>
               <operand valueType="t:Integer" value="2" xsi:type="Literal"/>
            </where>
         </expression>
      </def>
      <def name="Encounter with Comfort Measures during Day of or Day After Arrival" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="ComfortMeasure" xsi:type="With">
               <expression name="Intervention Comfort Measures" libraryName="TJC" xsi:type="ExpressionRef"/>
               <suchThat precision="Day" xsi:type="In">
                  <operand xsi:type="Coalesce">
                     <operand xsi:type="Start">
                        <operand name="ToInterval" libraryName="FHIRCommon" xsi:type="FunctionRef">
                           <operand path="FHIRHelpers.ToValue(%value)" scope="ComfortMeasure" xsi:type="Property"/>
                        </operand>
                     </operand>
                     <operand path="%value.value" scope="ComfortMeasure" xsi:type="Property"/>
                  </operand>
                  <operand name="CalendarDayOfOrDayAfter" libraryName="TJC" xsi:type="FunctionRef">
                     <operand xsi:type="Start">
                        <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                           <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                        </operand>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Encounter with Comfort Measure Procedure during Day of or Day After Arrival" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="ComfortProcedure" xsi:type="With">
               <expression dataType="fhir:Procedure" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-procedure" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                  <codes name="Comfort Measures" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="In">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="ComfortProcedure" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="List">
                        <element valueType="t:String" value="completed" xsi:type="Literal"/>
                        <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                     </operand>
                  </operand>
                  <operand precision="Day" xsi:type="In">
                     <operand xsi:type="Start">
                        <operand name="ToInterval" libraryName="FHIRCommon" xsi:type="FunctionRef">
                           <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="performed" scope="ComfortProcedure" xsi:type="Property"/>
                           </operand>
                        </operand>
                     </operand>
                     <operand name="CalendarDayOfOrDayAfter" libraryName="TJC" xsi:type="FunctionRef">
                        <operand xsi:type="Start">
                           <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                              <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Encounter with Comfort Measure ServiceRequest during Day of or Day After Arrival" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="ComfortServiceRequest" xsi:type="With">
               <expression dataType="fhir:ServiceRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-servicerequest" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                  <codes name="Comfort Measures" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="And">
                     <operand xsi:type="Equal">
                        <operand path="value" xsi:type="Property">
                           <source path="intent" scope="ComfortServiceRequest" xsi:type="Property"/>
                        </operand>
                        <operand valueType="t:String" value="order" xsi:type="Literal"/>
                     </operand>
                     <operand xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="status" scope="ComfortServiceRequest" xsi:type="Property"/>
                        </operand>
                        <operand xsi:type="List">
                           <element valueType="t:String" value="active" xsi:type="Literal"/>
                           <element valueType="t:String" value="on-hold" xsi:type="Literal"/>
                           <element valueType="t:String" value="completed" xsi:type="Literal"/>
                        </operand>
                     </operand>
                  </operand>
                  <operand precision="Day" xsi:type="In">
                     <operand path="value" xsi:type="Property">
                        <source path="authoredOn" scope="ComfortServiceRequest" xsi:type="Property"/>
                     </operand>
                     <operand name="CalendarDayOfOrDayAfter" libraryName="TJC" xsi:type="FunctionRef">
                        <operand xsi:type="Start">
                           <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                              <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Thrombolytic Therapy Medication or Procedures" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand xsi:type="Query">
               <source alias="ThrombolyticMedication">
                  <expression xsi:type="Union">
                     <operand dataType="fhir:MedicationAdministration" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationadministration" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Thrombolytic (t-PA) Therapy" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                     <operand dataType="fhir:MedicationAdministration" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationadministration" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                        <codes name="Thrombolytic (t-PA) Therapy" preserve="true" xsi:type="ValueSetRef"/>
                     </operand>
                  </expression>
               </source>
               <where xsi:type="In">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="ThrombolyticMedication" xsi:type="Property"/>
                  </operand>
                  <operand xsi:type="List">
                     <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                     <element valueType="t:String" value="completed" xsi:type="Literal"/>
                  </operand>
               </where>
               <return>
                  <expression xsi:type="Tuple">
                     <element name="id">
                        <value path="value" xsi:type="Property">
                           <source path="id" scope="ThrombolyticMedication" xsi:type="Property"/>
                        </value>
                     </element>
                     <element name="effective">
                        <value name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="effective" scope="ThrombolyticMedication" xsi:type="Property"/>
                        </value>
                     </element>
                  </expression>
               </return>
            </operand>
            <operand xsi:type="Query">
               <source alias="ThrombolyticProcedure">
                  <expression dataType="fhir:Procedure" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-procedure" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Intravenous or Intra-arterial Thrombolytic (t-PA) Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </expression>
               </source>
               <where xsi:type="Equal">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="ThrombolyticProcedure" xsi:type="Property"/>
                  </operand>
                  <operand valueType="t:String" value="completed" xsi:type="Literal"/>
               </where>
               <return>
                  <expression xsi:type="Tuple">
                     <element name="id">
                        <value path="value" xsi:type="Property">
                           <source path="id" scope="ThrombolyticProcedure" xsi:type="Property"/>
                        </value>
                     </element>
                     <element name="effective">
                        <value name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="performed" scope="ThrombolyticProcedure" xsi:type="Property"/>
                        </value>
                     </element>
                  </expression>
               </return>
            </operand>
         </expression>
      </def>
      <def name="Encounter with Thrombolytic Therapy Medication or Procedures" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="ThrombolyticTherapy" xsi:type="With">
               <expression name="Thrombolytic Therapy Medication or Procedures" xsi:type="ExpressionRef"/>
               <suchThat xsi:type="In">
                  <operand xsi:type="Start">
                     <operand name="ToInterval" libraryName="FHIRCommon" xsi:type="FunctionRef">
                        <operand path="effective" scope="ThrombolyticTherapy" xsi:type="Property"/>
                     </operand>
                  </operand>
                  <operand lowClosed="true" highClosed="false" xsi:type="Interval">
                     <low xsi:type="Subtract">
                        <operand xsi:type="Start">
                           <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                              <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                           </operand>
                        </operand>
                        <operand value="24" unit="hours" xsi:type="Quantity"/>
                     </low>
                     <high xsi:type="End">
                        <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                           <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                        </operand>
                     </high>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Encounter with Thrombolytic Therapy Prior to Arrival" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <where xsi:type="Exists">
               <operand xsi:type="Query">
                  <source alias="EncounterDiagnosis">
                     <expression name="EncounterDiagnosis" libraryName="CQMCommon" xsi:type="FunctionRef">
                        <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                     </expression>
                  </source>
                  <where xsi:type="InValueSet">
                     <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="code" scope="EncounterDiagnosis" xsi:type="Property"/>
                     </code>
                     <valueset name="Intravenous or Intra arterial Thrombolytic (tPA) Therapy Prior to Arrival" preserve="true"/>
                  </where>
               </operand>
            </where>
         </expression>
      </def>
      <def name="Encounter with Thrombolytic Therapy Documented As Already Given" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="PriorTPA" xsi:type="With">
               <expression dataType="fhir:Condition" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-condition" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                  <codes name="Intravenous or Intra arterial Thrombolytic (tPA) Therapy Prior to Arrival" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="And">
                     <operand xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="recordedDate" scope="PriorTPA" xsi:type="Property"/>
                        </operand>
                        <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                           <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                        </operand>
                     </operand>
                     <operand xsi:type="Or">
                        <operand xsi:type="Or">
                           <operand xsi:type="Equivalent">
                              <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="clinicalStatus" scope="PriorTPA" xsi:type="Property"/>
                              </operand>
                              <operand xsi:type="ToConcept">
                                 <operand name="active" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                              </operand>
                           </operand>
                           <operand xsi:type="Equivalent">
                              <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="clinicalStatus" scope="PriorTPA" xsi:type="Property"/>
                              </operand>
                              <operand xsi:type="ToConcept">
                                 <operand name="recurrence" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                              </operand>
                           </operand>
                        </operand>
                        <operand xsi:type="Equivalent">
                           <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="clinicalStatus" scope="PriorTPA" xsi:type="Property"/>
                           </operand>
                           <operand xsi:type="ToConcept">
                              <operand name="relapse" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
                  <operand xsi:type="Equivalent">
                     <operand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="verificationStatus" scope="PriorTPA" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="ToConcept">
                        <operand name="confirmed" libraryName="QICoreCommon" xsi:type="CodeRef"/>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Encounter with Thrombolytic Therapy Given Prior To Arrival Or During Hospitalization" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand xsi:type="Union">
               <operand name="Encounter with Thrombolytic Therapy Medication or Procedures" xsi:type="ExpressionRef"/>
               <operand name="Encounter with Thrombolytic Therapy Prior to Arrival" xsi:type="ExpressionRef"/>
            </operand>
            <operand name="Encounter with Thrombolytic Therapy Documented As Already Given" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="Denominator Exclusions" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand xsi:type="Union">
               <operand xsi:type="Union">
                  <operand name="Encounter Less Than Two Days" xsi:type="ExpressionRef"/>
                  <operand name="Encounter with Comfort Measures during Day of or Day After Arrival" xsi:type="ExpressionRef"/>
               </operand>
               <operand xsi:type="Union">
                  <operand name="Encounter with Comfort Measure Procedure during Day of or Day After Arrival" xsi:type="ExpressionRef"/>
                  <operand name="Encounter with Comfort Measure ServiceRequest during Day of or Day After Arrival" xsi:type="ExpressionRef"/>
               </operand>
            </operand>
            <operand name="Encounter with Thrombolytic Therapy Given Prior To Arrival Or During Hospitalization" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="Encounter with Antithrombotic Therapy" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="Antithrombotic" xsi:type="With">
               <expression xsi:type="Union">
                  <operand dataType="fhir:MedicationAdministration" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationadministration" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Antithrombotic Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
                  <operand dataType="fhir:MedicationAdministration" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationadministration" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Antithrombotic Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="In">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="Antithrombotic" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="List">
                        <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                        <element valueType="t:String" value="completed" xsi:type="Literal"/>
                     </operand>
                  </operand>
                  <operand precision="Day" xsi:type="In">
                     <operand xsi:type="Start">
                        <operand name="ToInterval" libraryName="FHIRCommon" xsi:type="FunctionRef">
                           <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="effective" scope="Antithrombotic" xsi:type="Property"/>
                           </operand>
                        </operand>
                     </operand>
                     <operand name="CalendarDayOfOrDayAfter" libraryName="TJC" xsi:type="FunctionRef">
                        <operand xsi:type="Start">
                           <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                              <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Numerator" context="Patient" accessLevel="Public">
         <expression name="Encounter with Antithrombotic Therapy" xsi:type="ExpressionRef"/>
      </def>
      <def name="Reason for Not Ordering Antithrombotic" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="NoAntithromboticOrder">
               <expression xsi:type="Union">
                  <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Antithrombotic Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
                  <operand dataType="fhir:MedicationRequest" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationrequest" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Antithrombotic Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="And">
                  <operand xsi:type="Equal">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="NoAntithromboticOrder" xsi:type="Property"/>
                     </operand>
                     <operand valueType="t:String" value="completed" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="IsTrue">
                     <operand path="value" xsi:type="Property">
                        <source path="doNotPerform" scope="NoAntithromboticOrder" xsi:type="Property"/>
                     </operand>
                  </operand>
               </operand>
               <operand xsi:type="Or">
                  <operand xsi:type="AnyInValueSet">
                     <codes xsi:type="Query">
                        <source alias="$this">
                           <expression path="reasonCode" scope="NoAntithromboticOrder" xsi:type="Property"/>
                        </source>
                        <return distinct="false">
                           <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </codes>
                     <valueset name="Medical Reason" preserve="true"/>
                  </operand>
                  <operand xsi:type="AnyInValueSet">
                     <codes xsi:type="Query">
                        <source alias="$this">
                           <expression path="reasonCode" scope="NoAntithromboticOrder" xsi:type="Property"/>
                        </source>
                        <return distinct="false">
                           <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </codes>
                     <valueset name="Patient Refusal" preserve="true"/>
                  </operand>
               </operand>
            </where>
            <return>
               <expression xsi:type="Tuple">
                  <element name="id">
                     <value path="value" xsi:type="Property">
                        <source path="id" scope="NoAntithromboticOrder" xsi:type="Property"/>
                     </value>
                  </element>
                  <element name="authoredOn">
                     <value path="value" xsi:type="Property">
                        <source path="authoredOn" scope="NoAntithromboticOrder" xsi:type="Property"/>
                     </value>
                  </element>
               </expression>
            </return>
         </expression>
      </def>
      <def name="Reason for Not Administering Antithrombotic" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="MedicationAdm">
               <expression dataType="fhir:MedicationAdministration" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-mednotadministered" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                  <codes name="Antithrombotic Therapy" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="Equal">
                  <operand path="value" xsi:type="Property">
                     <source path="status" scope="MedicationAdm" xsi:type="Property"/>
                  </operand>
                  <operand valueType="t:String" value="not-done" xsi:type="Literal"/>
               </operand>
               <operand xsi:type="Or">
                  <operand xsi:type="AnyInValueSet">
                     <codes xsi:type="Query">
                        <source alias="$this">
                           <expression path="statusReason" scope="MedicationAdm" xsi:type="Property"/>
                        </source>
                        <return distinct="false">
                           <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </codes>
                     <valueset name="Medical Reason" preserve="true"/>
                  </operand>
                  <operand xsi:type="AnyInValueSet">
                     <codes xsi:type="Query">
                        <source alias="$this">
                           <expression path="statusReason" scope="MedicationAdm" xsi:type="Property"/>
                        </source>
                        <return distinct="false">
                           <expression name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand name="$this" xsi:type="AliasRef"/>
                           </expression>
                        </return>
                     </codes>
                     <valueset name="Patient Refusal" preserve="true"/>
                  </operand>
               </operand>
            </where>
            <return>
               <expression xsi:type="Tuple">
                  <element name="id">
                     <value path="value" xsi:type="Property">
                        <source path="id" scope="MedicationAdm" xsi:type="Property"/>
                     </value>
                  </element>
                  <element name="authoredOn">
                     <value xsi:type="SingletonFrom">
                        <operand xsi:type="Query">
                           <source alias="$this">
                              <expression path="extension" xsi:type="Property">
                                 <source name="MedicationAdm" xsi:type="AliasRef"/>
                              </expression>
                           </source>
                           <where xsi:type="Equal">
                              <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="url" scope="$this" xsi:type="Property"/>
                              </operand>
                              <operand valueType="t:String" value="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-recorded" xsi:type="Literal"/>
                           </where>
                           <return distinct="false">
                              <expression path="value.value" xsi:type="Property">
                                 <source name="$this" xsi:type="AliasRef"/>
                              </expression>
                           </return>
                        </operand>
                     </value>
                  </element>
               </expression>
            </return>
         </expression>
      </def>
      <def name="No Antithrombotic Ordered or Administered" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand name="Reason for Not Ordering Antithrombotic" xsi:type="ExpressionRef"/>
            <operand name="Reason for Not Administering Antithrombotic" xsi:type="ExpressionRef"/>
         </expression>
      </def>
      <def name="No Antithrombotic Ordered or Administered Day Of or Day After Hospital Arrival" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="NoAntithrombotic" xsi:type="With">
               <expression name="No Antithrombotic Ordered or Administered" xsi:type="ExpressionRef"/>
               <suchThat precision="Day" xsi:type="In">
                  <operand path="authoredOn" scope="NoAntithrombotic" xsi:type="Property"/>
                  <operand name="CalendarDayOfOrDayAfter" libraryName="TJC" xsi:type="FunctionRef">
                     <operand xsi:type="Start">
                        <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                           <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                        </operand>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Encounter with Pharmacological Contraindications for Antithrombotic Therapy Given Day Of or Day After Hospital Arrival" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="PharmacologicalContraindications" xsi:type="With">
               <expression xsi:type="Union">
                  <operand dataType="fhir:MedicationAdministration" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationadministration" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Pharmacological Contraindications For Antithrombotic Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
                  <operand dataType="fhir:MedicationAdministration" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-medicationadministration" codeProperty="medication" codeComparator="in" xsi:type="Retrieve">
                     <codes name="Pharmacological Contraindications For Antithrombotic Therapy" preserve="true" xsi:type="ValueSetRef"/>
                  </operand>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="In">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="PharmacologicalContraindications" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="List">
                        <element valueType="t:String" value="in-progress" xsi:type="Literal"/>
                        <element valueType="t:String" value="completed" xsi:type="Literal"/>
                     </operand>
                  </operand>
                  <operand precision="Day" xsi:type="In">
                     <operand xsi:type="Start">
                        <operand name="ToInterval" libraryName="FHIRCommon" xsi:type="FunctionRef">
                           <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="effective" scope="PharmacologicalContraindications" xsi:type="Property"/>
                           </operand>
                        </operand>
                     </operand>
                     <operand name="CalendarDayOfOrDayAfter" libraryName="TJC" xsi:type="FunctionRef">
                        <operand xsi:type="Start">
                           <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                              <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Encounter with An INR Greater Than 3.5" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="IschemicStrokeEncounter">
               <expression name="Ischemic Stroke Encounter" libraryName="TJC" xsi:type="ExpressionRef"/>
            </source>
            <relationship alias="INR" xsi:type="With">
               <expression dataType="fhir:Observation" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-observation" codeProperty="code" codeComparator="in" xsi:type="Retrieve">
                  <codes name="INR" preserve="true" xsi:type="ValueSetRef"/>
               </expression>
               <suchThat xsi:type="And">
                  <operand xsi:type="And">
                     <operand precision="Day" xsi:type="In">
                        <operand path="value" xsi:type="Property">
                           <source path="issued" scope="INR" xsi:type="Property"/>
                        </operand>
                        <operand name="CalendarDayOfOrDayAfter" libraryName="TJC" xsi:type="FunctionRef">
                           <operand xsi:type="Start">
                              <operand name="HospitalizationWithObservation" libraryName="CQMCommon" xsi:type="FunctionRef">
                                 <operand name="IschemicStrokeEncounter" xsi:type="AliasRef"/>
                              </operand>
                           </operand>
                        </operand>
                     </operand>
                     <operand xsi:type="Greater">
                        <operand strict="false" xsi:type="As">
                           <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="value" scope="INR" xsi:type="Property"/>
                           </operand>
                           <asTypeSpecifier name="t:Quantity" xsi:type="NamedTypeSpecifier"/>
                        </operand>
                        <operand xsi:type="ToQuantity">
                           <operand valueType="t:Decimal" value="3.5" xsi:type="Literal"/>
                        </operand>
                     </operand>
                  </operand>
                  <operand xsi:type="In">
                     <operand path="value" xsi:type="Property">
                        <source path="status" scope="INR" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="List">
                        <element valueType="t:String" value="final" xsi:type="Literal"/>
                        <element valueType="t:String" value="amended" xsi:type="Literal"/>
                        <element valueType="t:String" value="corrected" xsi:type="Literal"/>
                     </operand>
                  </operand>
               </suchThat>
            </relationship>
         </expression>
      </def>
      <def name="Denominator Exceptions" context="Patient" accessLevel="Public">
         <expression xsi:type="Union">
            <operand xsi:type="Union">
               <operand name="No Antithrombotic Ordered or Administered Day Of or Day After Hospital Arrival" xsi:type="ExpressionRef"/>
               <operand name="Encounter with Pharmacological Contraindications for Antithrombotic Therapy Given Day Of or Day After Hospital Arrival" xsi:type="ExpressionRef"/>
            </operand>
            <operand name="Encounter with An INR Greater Than 3.5" xsi:type="ExpressionRef"/>
         </expression>
      </def>
