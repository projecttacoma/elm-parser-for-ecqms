      <def name="Meets Required Vaccination Threshold" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression xsi:type="GreaterOrEqual">
            <operand xsi:type="Count">
               <source xsi:type="Query">
                  <source alias="$this">
                     <expression xsi:type="Distinct">
                        <operand xsi:type="Query">
                           <source alias="Vaccine">
                              <expression name="VaccinationList" xsi:type="OperandRef"/>
                           </source>
                           <let identifier="vaccinationDate">
                              <expression name="Administered Date" xsi:type="FunctionRef">
                                 <operand name="Vaccine" xsi:type="AliasRef"/>
                              </expression>
                           </let>
                           <where xsi:type="In">
                              <operand name="vaccinationDate" xsi:type="QueryLetRef"/>
                              <operand xsi:type="Interval">
                                 <low xsi:type="ToDateTime">
                                    <operand path="low" xsi:type="Property">
                                       <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                    </operand>
                                 </low>
                                 <lowClosedExpression path="lowClosed" xsi:type="Property">
                                    <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                 </lowClosedExpression>
                                 <high xsi:type="ToDateTime">
                                    <operand path="high" xsi:type="Property">
                                       <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                    </operand>
                                 </high>
                                 <highClosedExpression path="highClosed" xsi:type="Property">
                                    <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                 </highClosedExpression>
                              </operand>
                           </where>
                           <return>
                              <expression xsi:type="Tuple">
                                 <element name="i">
                                    <value path="%value.value" scope="Vaccine" xsi:type="Property"/>
                                 </element>
                                 <element name="code">
                                    <value xsi:type="If">
                                       <condition xsi:type="Is">
                                          <operand name="Vaccine" xsi:type="AliasRef"/>
