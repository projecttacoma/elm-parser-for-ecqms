   <statements>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand dataType="fhir:Patient" templateId="http://hl7.org/fhir/us/qicore/StructureDefinition/qicore-patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="Diagnosis on or before Latest Possible Vaccination Date" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression xsi:type="Query">
            <source alias="ExclusionDiagnosis">
               <expression name="Diagnosis" xsi:type="OperandRef"/>
            </source>
            <where xsi:type="SameOrBefore">
               <operand xsi:type="Start">
                  <operand name="ToPrevalenceInterval" libraryName="QICoreCommon" xsi:type="FunctionRef">
                     <operand name="ExclusionDiagnosis" xsi:type="AliasRef"/>
                  </operand>
               </operand>
               <operand xsi:type="ToDateTime">
                  <operand name="LastVaccineDate" xsi:type="OperandRef"/>
               </operand>
            </where>
         </expression>
         <operand name="Diagnosis">
            <operandTypeSpecifier xsi:type="ListTypeSpecifier">
               <elementType name="fhir:Condition" xsi:type="NamedTypeSpecifier"/>
            </operandTypeSpecifier>
         </operand>
         <operand name="LastVaccineDate">
            <operandTypeSpecifier name="t:Date" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="Administered Date" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression xsi:type="Case">
            <caseItem>
               <when xsi:type="Is">
                  <operand name="Vaccination" xsi:type="OperandRef"/>
                  <isTypeSpecifier name="fhir:Immunization" xsi:type="NamedTypeSpecifier"/>
               </when>
               <then xsi:type="Start">
                  <operand name="ToInterval" libraryName="QICoreCommon" xsi:type="FunctionRef">
                     <operand xsi:type="As">
                        <operand path="value" xsi:type="Property">
                           <source path="occurrence" xsi:type="Property">
                              <source strict="false" xsi:type="As">
                                 <operand name="Vaccination" xsi:type="OperandRef"/>
                                 <asTypeSpecifier name="fhir:Immunization" xsi:type="NamedTypeSpecifier"/>
                              </source>
                           </source>
                        </operand>
                        <asTypeSpecifier xsi:type="ChoiceTypeSpecifier">
                           <choice name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
                           <choice name="t:Quantity" xsi:type="NamedTypeSpecifier"/>
                           <choice xsi:type="IntervalTypeSpecifier">
                              <pointType name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
                           </choice>
                           <choice xsi:type="IntervalTypeSpecifier">
                              <pointType name="t:Quantity" xsi:type="NamedTypeSpecifier"/>
                           </choice>
                        </asTypeSpecifier>
                     </operand>
                  </operand>
               </then>
            </caseItem>
            <caseItem>
               <when xsi:type="Is">
                  <operand name="Vaccination" xsi:type="OperandRef"/>
                  <isTypeSpecifier name="fhir:Procedure" xsi:type="NamedTypeSpecifier"/>
               </when>
               <then xsi:type="End">
                  <operand name="ToInterval" libraryName="QICoreCommon" xsi:type="FunctionRef">
                     <operand name="ToValue" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="performed" xsi:type="Property">
                           <source strict="false" xsi:type="As">
                              <operand name="Vaccination" xsi:type="OperandRef"/>
                              <asTypeSpecifier name="fhir:Procedure" xsi:type="NamedTypeSpecifier"/>
                           </source>
                        </operand>
                     </operand>
                  </operand>
               </then>
            </caseItem>
            <else asType="t:DateTime" xsi:type="As">
               <operand xsi:type="Null"/>
            </else>
         </expression>
         <operand name="Vaccination">
            <operandTypeSpecifier xsi:type="ChoiceTypeSpecifier">
               <choice name="fhir:Immunization" xsi:type="NamedTypeSpecifier"/>
               <choice name="fhir:Procedure" xsi:type="NamedTypeSpecifier"/>
            </operandTypeSpecifier>
         </operand>
      </def>
      <def name="Meets Required Vaccination Threshold with 14-Day Rule" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
