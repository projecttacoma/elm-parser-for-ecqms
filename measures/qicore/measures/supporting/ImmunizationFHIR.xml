                                                </lowClosedExpression>
                                                <high xsi:type="ToDateTime">
                                                   <operand path="high" xsi:type="Property">
                                                      <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                                   </operand>
                                                </high>
                                                <highClosedExpression path="highClosed" xsi:type="Property">
                                                   <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                                </highClosedExpression>
                                             </operand>
                                          </operand>
                                       </operand>
                                       <operand xsi:type="In">
                                          <operand name="VaccineDate3" xsi:type="QueryLetRef"/>
                                          <operand xsi:type="Interval">
                                             <low xsi:type="ToDateTime">
                                                <operand path="low" xsi:type="Property">
                                                   <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                                </operand>
                                             </low>
                                             <lowClosedExpression path="lowClosed" xsi:type="Property">
                                                <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                             </lowClosedExpression>
                                             <high xsi:type="ToDateTime">
                                                <operand path="high" xsi:type="Property">
                                                   <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                                </operand>
                                             </high>
                                             <highClosedExpression path="highClosed" xsi:type="Property">
                                                <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                             </highClosedExpression>
                                          </operand>
                                       </operand>
                                    </operand>
                                    <operand xsi:type="SameOrBefore">
                                       <operand name="VaccineDate1" xsi:type="QueryLetRef"/>
                                       <operand xsi:type="Subtract">
                                          <operand name="VaccineDate2" xsi:type="QueryLetRef"/>
                                          <operand value="14" unit="days" xsi:type="Quantity"/>
                                       </operand>
                                    </operand>
                                 </operand>
                                 <operand xsi:type="SameOrBefore">
                                    <operand name="VaccineDate2" xsi:type="QueryLetRef"/>
                                    <operand xsi:type="Subtract">
                                       <operand name="VaccineDate3" xsi:type="QueryLetRef"/>
                                       <operand value="14" unit="days" xsi:type="Quantity"/>
                                    </operand>
                                 </operand>
                              </where>
                              <return>
                                 <expression name="Vaccine1" xsi:type="AliasRef"/>
                              </return>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
                  <operand xsi:type="And">
                     <operand xsi:type="Equal">
                        <operand name="MinimumThreshold" xsi:type="OperandRef"/>
                        <operand valueType="t:Integer" value="4" xsi:type="Literal"/>
                     </operand>
                     <operand xsi:type="Exists">
                        <operand xsi:type="Query">
                           <source alias="Vaccine2">
                              <expression name="VaccinationList" xsi:type="OperandRef"/>
                           </source>
                           <source alias="Vaccine3">
                              <expression name="VaccinationList" xsi:type="OperandRef"/>
                           </source>
                           <source alias="Vaccine4">
                              <expression name="VaccinationList" xsi:type="OperandRef"/>
                           </source>
                           <let identifier="VaccineDate2">
                              <expression name="Administered Date" xsi:type="FunctionRef">
                                 <operand name="Vaccine2" xsi:type="AliasRef"/>
                              </expression>
                           </let>
                           <let identifier="VaccineDate3">
                              <expression name="Administered Date" xsi:type="FunctionRef">
                                 <operand name="Vaccine3" xsi:type="AliasRef"/>
                              </expression>
                           </let>
                           <let identifier="VaccineDate4">
                              <expression name="Administered Date" xsi:type="FunctionRef">
                                 <operand name="Vaccine4" xsi:type="AliasRef"/>
                              </expression>
                           </let>
                           <where xsi:type="And">
                              <operand xsi:type="And">
                                 <operand xsi:type="And">
                                    <operand xsi:type="And">
                                       <operand xsi:type="And">
                                          <operand xsi:type="And">
                                             <operand xsi:type="In">
                                                <operand name="VaccineDate1" xsi:type="QueryLetRef"/>
                                                <operand xsi:type="Interval">
                                                   <low xsi:type="ToDateTime">
                                                      <operand path="low" xsi:type="Property">
                                                         <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                                      </operand>
                                                   </low>
                                                   <lowClosedExpression path="lowClosed" xsi:type="Property">
                                                      <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                                   </lowClosedExpression>
                                                   <high xsi:type="ToDateTime">
                                                      <operand path="high" xsi:type="Property">
                                                         <source name="VaccinationPeriod" xsi:type="OperandRef"/>
                                                      </operand>
                                                   </high>
