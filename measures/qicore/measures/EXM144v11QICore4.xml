                           <operand name="Substance with beta adrenergic receptor antagonist mechanism of action (substance)" xsi:type="CodeRef"/>
                        </codes>
                     </operand>
                  </expression>
               </source>
               <where name="overlapsAfterHeartFailureOutpatientEncounter" libraryName="AHA" xsi:type="FunctionRef">
                  <operand xsi:type="As">
                     <operand name="BetaBlockerAllergyIntolerance" xsi:type="AliasRef"/>
