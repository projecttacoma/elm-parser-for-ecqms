               </source>
               <where name="overlapsAfterHeartFailureOutpatientEncounter" libraryName="AHA" xsi:type="FunctionRef">
                  <operand xsi:type="As">
                     <operand name="CardiacPacerDiagnosis" xsi:type="AliasRef"/>
                     <asTypeSpecifier xsi:type="ChoiceTypeSpecifier">
                        <choice name="fhir:Condition" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:Procedure" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:AllergyIntolerance" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:MedicationRequest" xsi:type="NamedTypeSpecifier"/>
                        <choice name="fhir:Observation" xsi:type="NamedTypeSpecifier"/>
                     </asTypeSpecifier>
                  </operand>
               </where>
            </operand>
         </expression>
      </def>
      <def name="Has Cardiac Pacer Device Implanted" context="Patient" accessLevel="Public">
