            <displayName value="Reference"/>
         </code>
         <value mediaType="text/plain"
                value="CITATIONAmerican Diabetes Association. Hypoglycemia (Low Blood Glucose). 2015; http://diabetes.org/living-with-diabetes/treatment-and-care/blood-glucose-control/hypoglycemia-low-blood.html. Accessed August 20, 2018."
                xsi:type="ED"/>
      </measureAttribute>
   </subjectOf>
   <subjectOf>
      <measureAttribute>
